.index-page {
  min-height: 100vh;
  background: #fff;
}
.carousel {
  height: 320rpx;
  background: #fff;
}
.carousel-img {
  width: 100%;
}


.count-down {
  width: 710rpx;
  margin: 20rpx auto 0;
  padding: 10rpx;
  border-radius: 12rpx;
  background-image: linear-gradient(to right, #fa1f86, #0462ea);
}
.count-down-box {
  padding: 20rpx 0rpx;
  border-radius: 8rpx;
  background: #fff;
}
.cound-down-box-title {
  padding-top: 10rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #f80;
  line-height: 1;
}
.cound-down-box-body {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  margin-top: 20rpx;
}
.cound-down-box-body-left {
  position: relative;
  width: 240rpx;
}
.cound-down-box-body-left::after {
  content: '';
  position: absolute;
  right: 0;
  top: 0;
  width: 2rpx;
  height: 100%;
  background: #fa1f86;
}
.cound-down-box-body-left-date {
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
  font-weight: bold;
}
.cound-down-box-body-left-remaining-time {
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
  font-weight: bold;
  color: #f80;
}
.cound-down-box-body-right {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  margin-left: 20rpx;
  flex: 1 0 0;
}
.cound-down-box-body-right-countdown {
  flex-shrink: 0;
  font-weight: bold;
  font-size: 40rpx;
  line-height: 100%;
  background-clip: text;
  -webkit-background-clip: text;
  background-image: linear-gradient(to right, #fa1f86, #0462ea);
  color: transparent;
}
.cound-down-box-body-right-signup {
  flex-shrink: 0;
  width: 210rpx;
  padding: 10rpx 0;
  margin-right: 20rpx;
  border-radius: 6rpx;
  background: rgba(4, 98, 234, 1);
  color: #fff;
  font-size: 30rpx;
  text-align: center;
}
.cound-down-box-body-right-signup:active {
  background: #1d78fb;
}
.count-down-friendly-tips {
  margin-top: 14rpx;
  font-size: 20rpx;
  color: #444;
  text-align: center;
}

/* 课程大纲 */
.course {
  margin-top: 20rpx;
  padding: 20rpx 0;
}
.course-title {
  color: rgb(0, 61, 167);
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.course-list {
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
  align-content: flex-start;
  margin: auto 8rpx;
}
.course-list-item {
  position: relative;
  width: calc(33.3333% - 16rpx);
  height: 240rpx;
  margin: 16rpx 8rpx 0;
  font-size: 28rpx;
  color: #fff;
  background: #0462ea;
  border-radius: 10rpx;
}
.course-list-item:active {
  background: #1d78fb;
}
.course-list-item-icon {
  position: relative;
  height: 100rpx;
  margin: 30rpx auto;
  text-align: center;
  opacity: 0.8;
}
.course-list-item-title {
  position: relative;
  font-size: 32rpx;
  line-height: 1;
  font-weight: bold;
  text-align: center;
}
.course-list-item-title::after {
  content: '';
  position: absolute;
  bottom: -15rpx;
  left: 0;
  right: 0;
  width: 100rpx;
  height: 2rpx;
  margin: auto;
  background: #fff;
}

/* 行业薪资 */
.industry-price {
  padding: 20rpx;
  background: #0462ea;
}
.industry-price-title {
  padding-top: 20rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.industry-price-content {
  padding: 30rpx 20rpx;
  margin: 20rpx auto 0;
  background: rgba(255, 255, 255, 0.7);
  border-radius: 10rpx;
}
.industry-price-content-item {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 40rpx;
}
.industry-price-content-item  + .industry-price-content-item {
  margin-top: 30rpx;
}
.industry-price-content-item-year, .industry-price-content-item-price {
  flex-shrink: 0;
  font-size: 30rpx;
  color: #333;
  line-height: 1;
}
.industry-price-content-item-year {
  width: 100rpx;
}
.industry-price-content-item-progressbar {
  width: 400rpx;
  height: 20rpx;
  margin: 0 10rpx;
  background: rgba(0, 0, 0, 0.1);
  border-radius: 10rpx;
  overflow: hidden;
}
.industry-price-content-item-progressbar-thumb {
  position: relative;
  width: 0;
  height: 20rpx;
  background: #f80;
  border-radius: 10rpx;
  overflow: hidden;
}

/* 培训生态链 */
.characteristic {
  padding: 20rpx;
  background: #0462ea;
}
.characteristic-title {
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.characteristic-item {
  margin: 20rpx 0 0;
  padding: 30rpx 20rpx;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 10rpx;
}
.characteristic-item-title {
  font-size: 30rpx;
  color: #f80;
  font-weight: bold;
  line-height: 40rpx;
}
.characteristic-item-context {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #333;
  line-height: 1.5;
}

/* 教学特色 */
.advantages {
  padding: 20rpx;
}
.advantages-title {
  padding: 20rpx;
  color: rgba(4, 98, 234, 1);
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
}
.advantages-item {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 20rpx;
  background: rgba(4, 98, 234, 0.9);
  border-radius: 10rpx;
}
.advantages-item + .advantages-item {
  margin-top: 20rpx;
}
.advantages-item-left {
  width: 100rpx;
}
.advantages-item-right {
  flex: 1 0 0;
  font-size: 28rpx;
  color: #f8f8f8;
  line-height: 1.5;
}

/* 教师资源 */
.teacher {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 20rpx;
  background: #0462ea;
}
.teacher-left {
  width: 140rpx;
  height: 140rpx;
  border: 4rpx solid #fff;
  border-radius: 50%;
  overflow: hidden;
}
.teacher-left-avatar {
  display: block;
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.teacher-right {
  flex: 1 0 0;
  margin-left: 30rpx;
}
.teacher-right-name {
  padding-top: 14rpx;
  font-size: 30rpx;
  line-height: 1;
  color: #fff;
}
.teacher-right-introduce {
  margin-top: 20rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 1.5;
}

/* 关于艾特 */
.about {
  position: relative;
  padding: 20rpx 0;
  background-color: #fff;
}
.about-bg {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  filter: blur(16px);
  opacity: 0.2;
}
.about-title {
  padding: 20rpx;
  font-size: 32rpx;
  line-height: 1;
  font-weight: bold;
  color: rgb(0, 61, 167);
}
.about-context {
  padding: 0 20rpx;
  font-size: 24rpx;
  color: black;
  line-height: 1.5;
}
.about-context-section {
  margin-top: 10rpx;
  text-indent: 2em;
}

.address {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 60rpx;
  margin-top: 10rpx;
  padding: 0 20rpx;
  padding-bottom: calc(20rpx + constant(safe-area-inset-bottom));
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
}
.address-text {
  font-size: 28rpx;
  color: #999;
}







